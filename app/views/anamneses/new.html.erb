<html>
	<body>
		<div class='container text-center col-md-7 panel'>
			<div class='row'>
				<h1 class='col-md-12' style='color: #70C4FF;'>
					Formul√°rio Anamnese
				</h1>
			</div>

			<div class='row justify-content-md-center'>
				<div class="col-md-11">
					<%= form_with model: @anamnese, local: true do |f| %>
						<% for field in @anamnese.attributes %>
							<% if not ['id','created_at', 'updated_at', 'user_id','athlete_id'].include? field[0] %>
							<p class='text-left'>
								<%= f.label field[0] %>
								<%= f.text_field field[0], class: 'form-control' %>
							</p>
							<% end %>
						<% end %>
						<p><%= f.submit 'Salvar', class: 'btn salvar', id:"btn-salvar", disabled: true %></p>
					<% end %>
				</div>
			</div>
		</div>
	</body>
</html>